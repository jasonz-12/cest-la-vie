{
    "openapi": "3.1.0",
    "info": {
      "title": "Content Management API",
      "description": "API for updating the habits of a day.",
      "version": "v1.0.0"
    },
    "servers": [
      {
        "url": "https://mvp-v1-habits-update-q5yqkjuiza-uk.a.run.app"
      }
    ],
    "paths": {
      "/receive-gpt-data": {
        "post": {
          "operationId": "habitsUpdate",
          "summary": "Update the relevant habits everyday.",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ContentItem"
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Content item received successfully"
            },
            "400": {
              "description": "Invalid request format"
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "ContentItem": {
          "type": "object",
          "properties": {
            "database_id": {
              "type": "string"
            },
            "integration_key": {
              "type": "string"
            },
            "specificDate": {
              "type": "string"
            },
            "properties": {
              "EatHealthy": {
                "type": "boolean"
              },
              "Exercise": {
                "type": "boolean"
              },
              "Hydration": {
                "type": "boolean"
              },
              "Meditation": {
                "type": "boolean"
              },
              "Reading": {
                "type": "boolean"
              }
            }
          },
          "required": ["properties", "database_id", "integration_key"]
        }
      }
    }
  }
  