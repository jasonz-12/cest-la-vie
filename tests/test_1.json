{
    "openapi": "3.1.0",
    "info": {
      "title": "Notion API Integration",
      "description": "Writes a new page to an existing Notion database.",
      "version": "v1.0.0"
    },
    "servers": [
      {
        "url": "https://api.notion.com/v1"
      }
    ],
    "paths": {
      "/pages/": {
        "post": {
            "description": "Writes a new page to an existing Notion database with specs.",
            "operationId": "createPage",
            "required": true,
            "tags": [
                "Pages"
            ],
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "$ref": "#/components/schemas/PageCreateRequest",
                            "properties": {
                                "parent": {
                                    "type": "object",
                                    "database_id": {
                                        "type": "string",
                                        "default": "618a8b62c258445dab5ade7dcfc0a518"
                                    }
                                },
                                "icon": {
                                    "type": "object",
                                    "properties": {
                                        "emoji": {
                                            "type": "string",
                                            "default": "üçÄ"
                                        }
                                    }
                                },
                                "cover": {
                                    "type": "object",
                                    "properties": {
                                        "external": {
                                            "type": "object",
                                            "properties": {
                                                "url": {
                                                    "type": "string",
                                                    "default": "https://unsplash.com/photos/a-couple-of-tall-buildings-next-to-each-other-9bLnvxJcxgg"
                                                }
                                            }
                                        }
                                    }
                                },
                                "properties": {
                                    "type": "object",
                                    "entry_title": {
                                        "type": "array",
                                        "title": [
                                            {
                                                "text": {
                                                    "content": "Shift to MVP Approach"
                                                }
                                            }
                                        ]
                                    },
                                    "tags": {
                                        "type": "array",
                                        "multi_select": [
                                            {
                                                "name": "principle"
                                            }
                                        ]
                                    },
                                    "entry_date": {
                                        "type": "string",
                                        "date": {
                                            "start": "2023-12-07"
                                        }
                                    }
                                },
                                "children": {
                                    "type": "object",
                                    "items": {
                                        "type": "array",
                                        "properties": [
                                            {
                                                "object": "block",
                                                "type": "heading_1",
                                                "heading_1": {
                                                    "rich_text": [
                                                        {
                                                            "type": "text",
                                                            "text": {
                                                                "content": "Development Updates"
                                                            }
                                                        }
                                                    ]
                                                }
                                            },
                                            {
                                                "object": "block",
                                                "type": "paragraph",
                                                "paragraph": {
                                                    "rich_text": [
                                                        {
                                                            "type": "text",
                                                            "text": {
                                                                "content": "Today is a good day!"
                                                            }
                                                        }
                                                    ]
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "in": "header",
                    "name": "Notion-Version",
                    "required": true,
                    "schema": {
                        "type": "string",
                        "default": "2022-06-28"
                    },
                    "description": "Version of the Notion API that's being used."
                }
            ],
            "responses": {
                "200": {
                    "description": "Page creation successful.",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PageResponse"
                            }
                        }
                    }
                },
                "400": {
                    "description": "Bad request, page not created.",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PageResponse"
                            }
                        }
                    }
                }
            }
        }
      }
    },
    "components": {
        "schemas": {
            "PageCreateRequest": {
                "type": "object",
                "properties": {
                    "parent": {
                        "type": "object",
                        "properties": {
                            "database_id": {
                                "type": "string",
                                "default": "618a8b62c258445dab5ade7dcfc0a518"
                            }
                        }
                    }
                }
            },
            "PageResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "created_time": {
                        "type": "string"
                    },
                    "last_edited_time": {
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "BearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "security": [
        {
            "BearerAuth": []
        }
    ]
  }